<odoo>
    <record id="view_crm_lead_structure_type_tree" model="ir.ui.view">
        <field name="name">crm.lead.structure.type.tree</field>
        <field name="model">crm.lead.structure.type</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
            </tree>
        </field>
    </record>

    <record id="view_crm_lead_structure_type_form" model="ir.ui.view">
        <field name="name">crm.lead.structure.type.form</field>
        <field name="model">crm.lead.structure.type</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <field name="name"/>
                </group>
            </form>
        </field>
    </record>

    <record id="action_crm_lead_structure_type" model="ir.actions.act_window">
        <field name="name">Lead Structure Type</field>
        <field name="res_model">crm.lead.structure.type</field>
        <field name="view_mode">tree,form</field>
    </record>

    <menuitem id="submenu_crm_lead_structure_type"
              name="Lead Structure Type"
              parent="menu_crm_solartree_group"
              action="action_crm_lead_structure_type"
              sequence="27"/>

</odoo>
