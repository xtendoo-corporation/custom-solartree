<odoo>
    <record id="view_crm_lead_channel_tree" model="ir.ui.view">
        <field name="name">crm.lead.channel.tree</field>
        <field name="model">crm.lead.channel</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
            </tree>
        </field>
    </record>

    <record id="view_crm_lead_channel_form" model="ir.ui.view">
        <field name="name">crm.lead.channel.form</field>
        <field name="model">crm.lead.channel</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <field name="name"/>
                </group>
            </form>
        </field>
    </record>

    <record id="action_crm_lead_channel" model="ir.actions.act_window">
        <field name="name">Lead Channel</field>
        <field name="res_model">crm.lead.channel</field>
        <field name="view_mode">tree,form</field>
    </record>

    <menuitem id="submenu_crm_lead_channel"
              name="Lead Channel"
              parent="menu_crm_solartree_group"
              action="action_crm_lead_channel"
              sequence="25"/>

</odoo>
