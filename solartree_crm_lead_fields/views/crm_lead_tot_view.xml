<odoo>
    <record id="view_crm_lead_tot_tree" model="ir.ui.view">
        <field name="name">crm.lead.tot.tree</field>
        <field name="model">crm.lead.tot</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
            </tree>
        </field>
    </record>

    <record id="view_crm_lead_tot_form" model="ir.ui.view">
        <field name="name">crm.lead.tot.form</field>
        <field name="model">crm.lead.tot</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <field name="name"/>
                </group>
            </form>
        </field>
    </record>

    <record id="action_crm_lead_tot" model="ir.actions.act_window">
        <field name="name">Lead TOT</field>
        <field name="res_model">crm.lead.tot</field>
        <field name="view_mode">tree,form</field>
    </record>

    <menuitem id="submenu_crm_lead_tot"
              name="Lead TOT"
              parent="menu_crm_solartree_group"
              action="action_crm_lead_tot"
              sequence="30"/>

</odoo>
