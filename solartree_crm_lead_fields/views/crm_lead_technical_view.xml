<odoo>
    <record id="view_crm_lead_technical_tree" model="ir.ui.view">
        <field name="name">crm.lead.technical.tree</field>
        <field name="model">crm.lead.technical</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
            </tree>
        </field>
    </record>

    <record id="view_crm_lead_technical_form" model="ir.ui.view">
        <field name="name">crm.lead.technical.form</field>
        <field name="model">crm.lead.technical</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <field name="name"/>
                </group>
            </form>
        </field>
    </record>

    <record id="action_crm_lead_technical" model="ir.actions.act_window">
        <field name="name">Lead Technical</field>
        <field name="res_model">crm.lead.technical</field>
        <field name="view_mode">tree,form</field>
    </record>

    <menuitem id="submenu_crm_lead_technical"
              name="Lead Technical"
              parent="menu_crm_solartree_group"
              action="action_crm_lead_technical"
              sequence="26"/>

</odoo>
