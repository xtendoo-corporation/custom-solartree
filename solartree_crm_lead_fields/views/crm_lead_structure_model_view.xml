<odoo>
    <record id="view_crm_lead_structure_model_tree" model="ir.ui.view">
        <field name="name">crm.lead.structure.model.tree</field>
        <field name="model">crm.lead.structure.model</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
            </tree>
        </field>
    </record>

    <record id="view_crm_lead_structure_model_form" model="ir.ui.view">
        <field name="name">crm.lead.structure.model.form</field>
        <field name="model">crm.lead.structure.model</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <field name="name"/>
                </group>
            </form>
        </field>
    </record>

    <record id="action_crm_lead_structure_model" model="ir.actions.act_window">
        <field name="name">Lead Structure Model</field>
        <field name="res_model">crm.lead.structure.model</field>
        <field name="view_mode">tree,form</field>
    </record>

    <menuitem id="submenu_crm_lead_structure_model"
              name="Lead Structure Model"
              parent="menu_crm_solartree_group"
              action="action_crm_lead_structure_model"
              sequence="28"/>

</odoo>
